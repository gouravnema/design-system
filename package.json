{
  "name": "@gouravnema/design-system",
  "version": "1.0.12",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npm run clean && rslib build --env-mode library && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json",
    "dev": "rslib dev --open",
    "format": "prettier --write .",
    "lint": "eslint .",
    "preview": "rslib preview",
    "storybook": "storybook dev -p6060 --no-open",
    "storybook:generate": "storybook automigrate --stories"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "classnames": "^2.5.1",
    "humanize-duration": "^3.33.0",
    "moment": "^2.30.1",
    "recharts": "^3.2.1",
    "styled-components": "^6.1.19",
    "react-router-dom": "^6.25.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@rslib/core": "^0.16.1",
    "@rsbuild/plugin-react": "^1.3.4",
    "@types/humanize-duration": "^3.27.4",
    "@types/moment": "^2.11.29",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "storybook-react-rsbuild": "^2.1.2",
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
